<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF

    <!--RDF-->

    <!--DHT11-->
    <res:DHT11 res:ID="DHT110000001">
        <sosa:isHostedBy rdf:resource="#ED0000001"/>
        <ssn-system:OperatingPowerRange>
            <schema:minValue>3<\schema:minValue>
            <schema:maxValue>5<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:V<\schema:unitCode>
        </ssn-system:OperatingPowerRange>
        <ssn-system:Frequency>
            <schema:value>1<\schema:value>
            <schema:unitCode>qudt-unit-1-1:Second<\schema:unitCode>
        </ssn-system:Frequency>
        <res:TempCondition>
            <schema:minValue>0<\schema:minValue>
            <schema:maxValue>50<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:DegreeCelsius<\schema:unitCode>
        </res:TempCondition>
        <res:HumCondition>
            <schema:minValue>20<\schema:minValue>
            <schema:maxValue>80<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:Percent<\schema:unitCode>
        </res:HumCondition>
        <res:DHT11TempAccuracy>
            <schema:minValue>-2<\schema:minValue>
            <schema:maxValue>2<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:DegreeCelsius<\schema:unitCode>
        </res:DHT11TempAccuracy>
        <res:DHT11HumAccuracy>
            <schema:value>5<\schema:value>
            <schema:unitCode>qudt-unit-1-1:Percent<\schema:unitCode>
        </res:DHT11HumAccuracy>
    </res:DHT11>

    <!--MQ2-->
    <res:MQ2 res:ID="MQ20000001">
        <sosa:isHostedBy rdf:resource="#ED0000001"/>
        <ssn-system:OperatingPowerRange>
            <schema:value>5<\schema:value>
            <schema:unitCode>qudt-unit-1-1:V<\schema:unitCode>
        </ssn-system:OperatingPowerRange>
        <ssn-system:Frequency>
            <schema:value>1<\schema:value>
            <schema:unitCode>qudt-unit-1-1:Second<\schema:unitCode>
        </ssn-system:Frequency>
        <res:GasMeasurementRange>
            <schema:minValue>200<\schema:minValue>
            <schema:maxValue>10000<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:PPM<\schema:unitCode>
        </res:GasMeasurementRange>
        <res:TempCondition>
            <schema:minValue>18<\schema:minValue>
            <schema:maxValue>22<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:DegreeCelsius<\schema:unitCode>
        </res:TempCondition>
        <res:HumCondition>
            <schema:minValue>60<\schema:minValue>
            <schema:maxValue>70<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:Percent<\schema:unitCode>
        </res:HumCondition>
        <res:MQ2OxygenCondition>
            <schema:value>21<\schema:value>
            <schema:unitCode>qudt-unit-1-1:Percent<\schema:unitCode>
        </res:MQ2OxygenCondition>
    </res:MQ2>

    <!--KY026-->
    <res:KY026 res:ID="KY0260000001">
        <sosa:isHostedBy rdf:resource="#ED0000001"/>
        <ssn-system:OperatingPowerRange>
            <schema:minValue>3<\schema:minValue>
            <schema:maxValue>5<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:V<\schema:unitCode>
        </ssn-system:OperatingPowerRange>
        <res:KY026UVMeasurementRange>
            <schema:minValue>760<\schema:minValue>
            <schema:maxValue>1100<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:NanoM<\schema:unitCode>
        </res:KY026UVMeasurementRange>
        <res:KY026AngleMeasurementRange>
            <schema:minValue>60<\schema:minValue>
            <schema:unitCode>qudt-unit-1-1:DEG<\schema:unitCode>
        </res:KY026AngleMeasurementRange>
    </res:KY026>

    <!--SFM27-->
    <res:SFM27 res:ID="SFM270000001">
        <sosa:isHostedBy rdf:resource="#ED0000001"/>
        <ssn-system:OperatingPowerRange>
            <schema:minValue>3<\schema:minValue>
            <schema:maxValue>24<\schema:maxValue>
            <schema:unitCode>qudt-unit-1-1:V<\schema:unitCode>
        </ssn-system:OperatingPowerRange>
        <res:SFM27SoundOutput>
            <schema:greaterOrEqual>95<\schema:greaterOrEqual>
            <schema:unitCode>qudt-unit-1-1:DeciB<\schema:unitCode>
        </res:SFM27SoundOutput>
    </res:SFM27>

    <!--EdgeDevice-->
    <res:EdgeDevice res:ID="ED0000001">
        <sosa:hosts>
            <rdf:Bag>
                <rdf:_1 rdf:resource="#DHT110000001"/>
                <rdf:_2 rdf:resource="#MQ20000001"/>
                <rdf:_3 rdf:resource="#KY0260000001"/>
                <rdf:_4 rdf:resource="#SFM270000001"/>
		    </rdf:Bag>
        </sosa:hosts>
        <geo:Point>
            <geo:lat>55.701</geo:lat>
            <geo:long>12.552</geo:long>
        </geo:Point>
        <res:isRoutedBy>
            <rdf:Bag>
                <!--List of router node that route data to the edge device ED0000001-->
                <rdf:_1 rdf:resource="#RN0000001"/>
                <rdf:_1 rdf:resource="#RN0000002"/>
                <rdf:_1 rdf:resource="#RN0000003"/>
		    </rdf:Bag>
        </res:isRoutedBy>
    </res:EdgeDevice>
</rdf:RDF>
