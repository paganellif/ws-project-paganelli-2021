@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt-1-1: <http://qudt.org/1.1/schema/qudt#> .
@prefix qudt-unit-1-1: <http://qudt.org/1.1/vocab/unit#> .
@prefix schema: <http://schema.org/>.
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix ssn-system: <http://www.w3.org/ns/ssn/systems/> .
@prefix rdfp: <https://w3id.org/rdfp/>.

@base <http://example.org/data/ky026> .

<KY026> a ssn:System ;
  rdfs:comment "KY-026 flame IR sensor."@en ;
  rdfs:seeAlso <https://moviltronics.com/wp-content/uploads/2019/10/KY-026.pdf> ;
  ssn-system:hasOperatingRange <KY026UltravioletSensorOperatingRange>, <KY026DetectionAngleOperatingRange>, <KY026SensorOperatingPowerRange> ;
  ssn-system:hasSystemProperty <KY026PowerConsumption> .

<KY026SensorOperatingPowerRange> a ssn-system:OperatingRange ;
  rdfs:comment "3.3 to 5V power"@en ;
  schema:minValue 3.3 ;
  schema:maxValue 5 ;
  schema:unitCode qudt-unit-1-1:V .

<KY026UltravioletSensorOperatingRange> a ssn-system:OperatingRange ;
  rdfs:comment "Ultraviolet Detection Range: 760nm to 1100nm"@en ;
  schema:minValue 760 ;
  schema:maxValue 1100 ;
  schema:unitCode qudt-unit-1-1:NanoM .

<KY026DetectionAngleOperatingRange> a ssn-system:OperatingRange ;
  rdfs:comment "Detection Angle: ~60 degrees"@en ;
  schema:value 60 ;
  schema:unitCode qudt-unit-1-1:DEG .

<KY026PowerConsumption> a schema:PropertyValue ;
  rdfs:comment "Maximum Current: 15mA"@en ;
  schema:maxValue 15 ;
  schema:unitCode qudt-unit-1-1:MilliA .
